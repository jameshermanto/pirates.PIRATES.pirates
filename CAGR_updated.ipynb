  {
   "cell_type": "code",
   "execution_count": 102,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = yf.download('indy.jk',\n",
    "                 start='2014-12-31', \n",
    "                 end='2019-12-31',\n",
    "                 actions='inline',\n",
    "                 progress=False)\n",
    "\n",
    "df = df.loc[:, ['Adj Close']]\n",
